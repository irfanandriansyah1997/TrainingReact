    "jest": {
        "collectCoverage": true,
        "testURL": "http://localhost/",
        "collectCoverageFrom": [
            "src/**/*.{js,jsx}",
            "!src/app.js",
            "!src/components/themes/**/*.{js,jsx}",
            "!src/**/*.stories.{js,jsx}",
            "!dist/**/*.js",
            "!dist/*.js",
            "!src/*.scss",
            "!src/*.less",
            "!src/*.svg"
        ],
        "moduleNameMapper": {
            "^.+\\.(css|less|scss)$": "babel-jest",
            "^@/(.*)$": "<rootDir>/src/$1",
            "^config/(.*)$": "<rootDir>/etc/jest/$1"
        },
        "coverageDirectory": ".jest_coverage",
        "moduleFileExtensions": [
            "js",
            "jsx",
            "es6"
        ],
        "transform": {
            "^.+\\.js$": "babel-jest",
            "^.+\\.jsx$": "babel-jest",
            "^.+\\.es6$": "babel-jest"
        },
        "transformIgnorePatterns": [
            "node_modules/"
        ]
    }
